<!DOCTYPE html>
<html lang="es">
<!-- Include Head -->
<%- include('./partials/head') %>
<body>
	<!-- Header -->
	<% if(locals.usuario){ %>
		<%- include('./partials/headerLogueado') %>
		<% } else{ %>
			<%- include('./partials/headerDeslogueado') %>
			<% } %>
	<!-- /Header -->
	
	<!-- Novedades -->
	<% console.log(autos) %>
	<div class="container products-wrapper">
		<div class="row">
			<div class="col-12">
				<h2 class="products-title">Resultados de b√∫squeda</h2>
			</div>
			<!-- Show the "ultimos" products -->
			<% for(let i=0; i<autos.length; i++){ %>
				<div class="col-12 col-sm-6 col-lg-3">
					<section class="product-box">
						<a href="/product/<%= autos[i].id %>">
							<figure class="product-box_image">
								<img src="images/products/<%= autos[i].nombre_foto %>" alt="cafetera moulinex">
							</figure>
							<article class="product-box_data">
								<h2><%= autos[i].nombre_producto %></h2>
								<p><%= autos[i].descripcion %> </p>
								<p>Comentarios: 12</p>
							</article>
						</a>
					</section>
				</div>
			<% } %>



			<h2 class="col-12">
				No hay resultados de busqueda.....
			</h2>


			<%- include('./partials/footer') %>

</body>

</html>