<!DOCTYPE html>
<html lang="es">
<!-- Include Head -->
<%- include('./partials/head') %>
<body>
	<!-- Header -->
	<% if(locals.usuario){ %>
		<%- include('./partials/headerLogueado') %>
		<% } else{ %>
			<%- include('./partials/headerDeslogueado') %>
			<% } %>
	<!-- /Header -->

	
	<!-- Novedades -->
	<div class="container products-wrapper">
		<div class="row">
			<div class="col-12">
				<h2 class="products-title">Agregar productos</h2>
			</div>
			<!-- Show the "ultimos" products -->
			<h2 class="col-12"> 
				<form action="/searchResults" method="GET">
					<div>
						<label for="image">Imagen:</label>
						<input id="image" type="file" name="image" value="">
					</div>
					<div>
						<label for="producto">Nombre del producto:</label>
						<input id="producto" type="text" name="producto" value="">
					</div>
					<div>
						<label for="descripcion">Descripcion:</label>
						<input id="descripcion" type="text" name="descripcion" value="">
					</div>
					<div>
						<button type="submit">Enviar</button>
					</div>
				</form>
			</h2>


			<%- include('./partials/footer') %>

</body>

</html>